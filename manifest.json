{   "manifest_version": 2,

    "name": "InfiniPassword",
    "description": "This extension is a free password manager.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "default.html"
    },
    
    "permissions": [
        "activeTab",
        "clipboard-write"
    ]
}